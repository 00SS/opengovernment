<% if (bill_search_results.length > 0) { %>
<ul id="searchResults">
  <% _.each(bill_search_results, function(bill) { %>
    <li><a class="billSearchResult" href="#" data-bill="<%= bill.to_param %>" data-session="<%= bill.session.to_param %>"><strong><%= bill.session.name_fm %> <%= bill.bill_number %></strong> <%= bill.title.substring(0, 50) + (bill.title.length > 50 ? "..." : '') %></a></li>
  <% }); %>
</div>
<% } else { %>
Sorry, no matching bills were found.
<% } %>